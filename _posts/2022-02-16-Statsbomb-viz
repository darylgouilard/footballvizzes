<span class="c1"># Display the number of blocked shots made by the home and away team</span>
<span class="n">ax</span><span class="o">.</span><span class="n">text</span><span class="p">(</span><span class="mi">60</span><span class="p">,</span> <span class="mi">54</span><span class="p">,</span> <span class="s1">&#39;Shots blocked&#39;</span><span class="p">,</span> <span class="n">color</span> <span class="o">=</span> <span class="s1">&#39;black&#39;</span><span class="p">,</span> <span class="n">font_properties</span> <span class="o">=</span> <span class="n">robotoBold</span><span class="p">,</span> <span class="n">fontsize</span> <span class="o">=</span> <span class="mi">12</span><span class="p">,</span> <span class="n">ha</span> <span class="o">=</span> <span class="s1">&#39;center&#39;</span><span class="p">,</span> <span class="n">bbox</span>  <span class="o">=</span> <span class="n">text_box</span><span class="p">)</span>
<span class="n">ax</span><span class="o">.</span><span class="n">text</span><span class="p">(</span><span class="mi">45</span><span class="p">,</span> <span class="mi">54</span><span class="p">,</span> <span class="nb">str</span><span class="p">(</span><span class="n">home_blocked</span><span class="p">),</span> <span class="n">color</span> <span class="o">=</span> <span class="s1">&#39;white&#39;</span><span class="p">,</span> <span class="n">font_properties</span> <span class="o">=</span> <span class="n">robotoBold</span><span class="p">,</span> <span class="n">fontsize</span> <span class="o">=</span> <span class="mi">12</span><span class="p">,</span> <span class="n">ha</span> <span class="o">=</span> <span class="s1">&#39;left&#39;</span><span class="p">,</span> <span class="n">bbox</span>  <span class="o">=</span> <span class="n">home_values</span><span class="p">)</span>
<span class="n">ax</span><span class="o">.</span><span class="n">text</span><span class="p">(</span><span class="mi">72</span><span class="p">,</span> <span class="mi">54</span><span class="p">,</span> <span class="nb">str</span><span class="p">(</span><span class="n">away_blocked</span><span class="p">),</span> <span class="n">color</span> <span class="o">=</span> <span class="n">away_edge_colour</span><span class="p">,</span> <span class="n">font_properties</span> <span class="o">=</span> <span class="n">robotoBold</span><span class="p">,</span> <span class="n">fontsize</span> <span class="o">=</span> <span class="mi">12</span><span class="p">,</span> <span class="n">ha</span> <span class="o">=</span> <span class="s1">&#39;left&#39;</span><span class="p">,</span> <span class="n">bbox</span>  <span class="o">=</span> <span class="n">away_values</span><span class="p">)</span>

<span class="c1"># Display the shapes that represent each shots being plotted on the pitch</span>
<span class="n">ax</span><span class="o">.</span><span class="n">text</span><span class="p">(</span><span class="mi">30</span><span class="p">,</span> <span class="o">-</span><span class="mf">0.9</span><span class="p">,</span> <span class="s1">&#39;Outcomes:&#39;</span><span class="p">,</span> <span class="n">color</span> <span class="o">=</span> <span class="s1">&#39;white&#39;</span><span class="p">,</span> <span class="n">font_properties</span> <span class="o">=</span> <span class="n">robotoBold</span><span class="p">,</span> <span class="n">fontsize</span> <span class="o">=</span> <span class="mi">15</span><span class="p">,</span> <span class="n">ha</span> <span class="o">=</span> <span class="s1">&#39;center&#39;</span><span class="p">)</span>

<span class="c1"># Circle with yellow outline to indicate goals</span>
<span class="n">nodes</span> <span class="o">=</span> <span class="n">pitch</span><span class="o">.</span><span class="n">scatter</span><span class="p">(</span><span class="mi">4</span><span class="p">,</span> <span class="mi">4</span><span class="p">,</span> <span class="n">s</span> <span class="o">=</span> <span class="mi">300</span><span class="p">,</span> <span class="n">marker</span> <span class="o">=</span> <span class="s1">&#39;o&#39;</span><span class="p">,</span>
                        <span class="n">color</span> <span class="o">=</span> <span class="n">home_colour</span><span class="p">,</span> <span class="n">edgecolors</span> <span class="o">=</span> <span class="s1">&#39;yellow&#39;</span><span class="p">,</span> <span class="n">zorder</span> <span class="o">=</span> <span class="mi">1</span><span class="p">,</span> <span class="n">ax</span> <span class="o">=</span> <span class="n">ax</span><span class="p">)</span>
<span class="n">ax</span><span class="o">.</span><span class="n">text</span><span class="p">(</span><span class="mf">8.5</span><span class="p">,</span> <span class="mf">4.25</span><span class="p">,</span> <span class="s1">&#39;Goal&#39;</span><span class="p">,</span> <span class="n">color</span> <span class="o">=</span> <span class="s1">&#39;white&#39;</span><span class="p">,</span> <span class="n">font_properties</span> <span class="o">=</span> <span class="n">robotoBold</span><span class="p">,</span> <span class="n">fontsize</span> <span class="o">=</span> <span class="mi">12</span><span class="p">,</span> <span class="n">ha</span> <span class="o">=</span> <span class="s1">&#39;center&#39;</span><span class="p">)</span>

<span class="c1"># Triangle to indicate shots on target</span>
<span class="n">nodes</span> <span class="o">=</span> <span class="n">pitch</span><span class="o">.</span><span class="n">scatter</span><span class="p">(</span><span class="mi">13</span><span class="p">,</span> <span class="mi">4</span><span class="p">,</span> <span class="n">s</span> <span class="o">=</span> <span class="mi">300</span><span class="p">,</span> <span class="n">marker</span> <span class="o">=</span> <span class="s1">&#39;^&#39;</span><span class="p">,</span>
                                <span class="n">color</span> <span class="o">=</span> <span class="n">away_colour</span><span class="p">,</span> <span class="n">edgecolors</span> <span class="o">=</span> <span class="n">away_edge_colour</span><span class="p">,</span> <span class="n">zorder</span> <span class="o">=</span> <span class="mi">1</span><span class="p">,</span> <span class="n">ax</span> <span class="o">=</span> <span class="n">ax</span><span class="p">)</span>
<span class="n">ax</span><span class="o">.</span><span class="n">text</span><span class="p">(</span><span class="mf">19.5</span><span class="p">,</span> <span class="mf">4.25</span><span class="p">,</span> <span class="s1">&#39;On target&#39;</span><span class="p">,</span> <span class="n">color</span> <span class="o">=</span> <span class="s1">&#39;white&#39;</span><span class="p">,</span> <span class="n">font_properties</span> <span class="o">=</span> <span class="n">robotoBold</span><span class="p">,</span> <span class="n">fontsize</span> <span class="o">=</span> <span class="mi">12</span><span class="p">,</span> <span class="n">ha</span> <span class="o">=</span> <span class="s1">&#39;center&#39;</span><span class="p">)</span>

<span class="c1"># Square to indicate shots that hit the post</span>
<span class="n">nodes</span> <span class="o">=</span> <span class="n">pitch</span><span class="o">.</span><span class="n">scatter</span><span class="p">(</span><span class="mf">26.5</span><span class="p">,</span> <span class="mi">4</span><span class="p">,</span> <span class="n">s</span> <span class="o">=</span> <span class="mi">300</span><span class="p">,</span> <span class="n">marker</span> <span class="o">=</span> <span class="s1">&#39;s&#39;</span><span class="p">,</span>
                        <span class="n">color</span> <span class="o">=</span> <span class="n">home_colour</span><span class="p">,</span> <span class="n">edgecolors</span> <span class="o">=</span> <span class="n">home_edge_colour</span><span class="p">,</span> <span class="n">zorder</span> <span class="o">=</span> <span class="mi">1</span><span class="p">,</span> <span class="n">ax</span> <span class="o">=</span> <span class="n">ax</span><span class="p">)</span>
<span class="n">ax</span><span class="o">.</span><span class="n">text</span><span class="p">(</span><span class="mf">32.7</span><span class="p">,</span> <span class="mf">4.25</span><span class="p">,</span> <span class="s1">&#39;Hit post&#39;</span><span class="p">,</span> <span class="n">color</span> <span class="o">=</span> <span class="s1">&#39;white&#39;</span><span class="p">,</span> <span class="n">font_properties</span> <span class="o">=</span> <span class="n">robotoBold</span><span class="p">,</span> <span class="n">fontsize</span> <span class="o">=</span> <span class="mi">12</span><span class="p">,</span> <span class="n">ha</span> <span class="o">=</span> <span class="s1">&#39;center&#39;</span><span class="p">)</span>

<span class="c1"># Diamond to indicate blocked shots</span>
<span class="n">nodes</span> <span class="o">=</span> <span class="n">pitch</span><span class="o">.</span><span class="n">scatter</span><span class="p">(</span><span class="mi">40</span><span class="p">,</span> <span class="mi">4</span><span class="p">,</span> <span class="n">s</span> <span class="o">=</span> <span class="mi">270</span><span class="p">,</span> <span class="n">marker</span> <span class="o">=</span> <span class="s1">&#39;D&#39;</span><span class="p">,</span>
                                <span class="n">color</span> <span class="o">=</span> <span class="n">away_colour</span><span class="p">,</span> <span class="n">edgecolors</span> <span class="o">=</span> <span class="n">away_edge_colour</span><span class="p">,</span> <span class="n">zorder</span> <span class="o">=</span> <span class="mi">1</span><span class="p">,</span> <span class="n">ax</span> <span class="o">=</span> <span class="n">ax</span><span class="p">)</span>
<span class="n">ax</span><span class="o">.</span><span class="n">text</span><span class="p">(</span><span class="mi">47</span><span class="p">,</span> <span class="mf">4.25</span><span class="p">,</span> <span class="s1">&#39;Blocked&#39;</span><span class="p">,</span> <span class="n">color</span> <span class="o">=</span> <span class="s1">&#39;white&#39;</span><span class="p">,</span> <span class="n">font_properties</span> <span class="o">=</span> <span class="n">robotoBold</span><span class="p">,</span> <span class="n">fontsize</span> <span class="o">=</span> <span class="mi">12</span><span class="p">,</span> <span class="n">ha</span> <span class="o">=</span> <span class="s1">&#39;center&#39;</span><span class="p">)</span>

<span class="c1"># X to indicate shots off target</span>
<span class="n">nodes</span> <span class="o">=</span> <span class="n">pitch</span><span class="o">.</span><span class="n">scatter</span><span class="p">(</span><span class="mf">53.5</span><span class="p">,</span> <span class="mi">4</span><span class="p">,</span> <span class="n">s</span> <span class="o">=</span> <span class="mi">300</span><span class="p">,</span> <span class="n">marker</span> <span class="o">=</span> <span class="s1">&#39;X&#39;</span><span class="p">,</span>
                        <span class="n">color</span> <span class="o">=</span> <span class="n">home_colour</span><span class="p">,</span> <span class="n">edgecolors</span> <span class="o">=</span> <span class="n">home_edge_colour</span><span class="p">,</span> <span class="n">zorder</span> <span class="o">=</span> <span class="mi">1</span><span class="p">,</span> <span class="n">ax</span> <span class="o">=</span> <span class="n">ax</span><span class="p">)</span>
<span class="n">ax</span><span class="o">.</span><span class="n">text</span><span class="p">(</span><span class="mf">60.5</span><span class="p">,</span> <span class="mf">4.25</span><span class="p">,</span> <span class="s1">&#39;Off target&#39;</span><span class="p">,</span> <span class="n">color</span> <span class="o">=</span> <span class="s1">&#39;white&#39;</span><span class="p">,</span> <span class="n">font_properties</span> <span class="o">=</span> <span class="n">robotoBold</span><span class="p">,</span> <span class="n">fontsize</span> <span class="o">=</span> <span class="mi">12</span><span class="p">,</span> <span class="n">ha</span> <span class="o">=</span> <span class="s1">&#39;center&#39;</span><span class="p">)</span>

<span class="c1"># Display the xG values, which indicate that the size of the shape increases by the xG value of the shot</span>
<span class="n">ax</span><span class="o">.</span><span class="n">text</span><span class="p">(</span><span class="mi">90</span><span class="p">,</span> <span class="o">-</span><span class="mf">0.9</span><span class="p">,</span> <span class="s1">&#39;xG values:&#39;</span><span class="p">,</span> <span class="n">color</span> <span class="o">=</span> <span class="s1">&#39;white&#39;</span><span class="p">,</span> <span class="n">font_properties</span> <span class="o">=</span> <span class="n">robotoBold</span><span class="p">,</span> <span class="n">fontsize</span> <span class="o">=</span> <span class="mi">15</span><span class="p">,</span> <span class="n">ha</span> <span class="o">=</span> <span class="s1">&#39;center&#39;</span><span class="p">)</span>
<span class="n">nodes</span> <span class="o">=</span> <span class="n">pitch</span><span class="o">.</span><span class="n">scatter</span><span class="p">(</span><span class="mf">82.5</span><span class="p">,</span> <span class="mi">4</span><span class="p">,</span> <span class="n">s</span> <span class="o">=</span> <span class="mi">100</span><span class="p">,</span> <span class="n">marker</span> <span class="o">=</span> <span class="s1">&#39;o&#39;</span><span class="p">,</span>
                        <span class="n">color</span> <span class="o">=</span> <span class="n">home_colour</span><span class="p">,</span> <span class="n">edgecolors</span> <span class="o">=</span> <span class="n">home_edge_colour</span><span class="p">,</span> <span class="n">zorder</span> <span class="o">=</span> <span class="mi">1</span><span class="p">,</span> <span class="n">ax</span> <span class="o">=</span> <span class="n">ax</span><span class="p">)</span>
<span class="n">nodes</span> <span class="o">=</span> <span class="n">pitch</span><span class="o">.</span><span class="n">scatter</span><span class="p">(</span><span class="mf">85.5</span><span class="p">,</span> <span class="mi">4</span><span class="p">,</span> <span class="n">s</span> <span class="o">=</span> <span class="mi">200</span><span class="p">,</span> <span class="n">marker</span> <span class="o">=</span> <span class="s1">&#39;o&#39;</span><span class="p">,</span>
                        <span class="n">color</span> <span class="o">=</span> <span class="n">away_colour</span><span class="p">,</span> <span class="n">edgecolors</span> <span class="o">=</span> <span class="n">away_edge_colour</span><span class="p">,</span> <span class="n">zorder</span> <span class="o">=</span> <span class="mi">1</span><span class="p">,</span> <span class="n">ax</span> <span class="o">=</span> <span class="n">ax</span><span class="p">)</span>
<span class="n">nodes</span> <span class="o">=</span> <span class="n">pitch</span><span class="o">.</span><span class="n">scatter</span><span class="p">(</span><span class="mf">89.2</span><span class="p">,</span> <span class="mi">4</span><span class="p">,</span> <span class="n">s</span> <span class="o">=</span> <span class="mi">300</span><span class="p">,</span> <span class="n">marker</span> <span class="o">=</span> <span class="s1">&#39;o&#39;</span><span class="p">,</span>
                        <span class="n">color</span> <span class="o">=</span> <span class="n">home_colour</span><span class="p">,</span> <span class="n">edgecolors</span> <span class="o">=</span> <span class="n">home_edge_colour</span><span class="p">,</span> <span class="n">zorder</span> <span class="o">=</span> <span class="mi">1</span><span class="p">,</span> <span class="n">ax</span> <span class="o">=</span> <span class="n">ax</span><span class="p">)</span>
