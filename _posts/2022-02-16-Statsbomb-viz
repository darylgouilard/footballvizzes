     <div class="CodeMirror cm-s-jupyter">
<div class=" highlight hl-ipython3"><pre><span></span><span class="c1"># Retrieve each half&#39;s length from the half_times list</span>
<span class="n">first_half_time</span> <span class="o">=</span> <span class="n">half_times</span><span class="p">[</span><span class="s2">&quot;minute&quot;</span><span class="p">][</span><span class="mi">0</span><span class="p">]</span>
<span class="n">second_half_time</span> <span class="o">=</span> <span class="n">half_times</span><span class="p">[</span><span class="s2">&quot;minute&quot;</span><span class="p">][</span><span class="mi">1</span><span class="p">]</span> <span class="o">-</span> <span class="mi">45</span>

<span class="c1"># If the match is played through the extra time...</span>
<span class="k">if</span> <span class="p">(</span><span class="nb">len</span><span class="p">(</span><span class="n">half_times</span><span class="p">)</span> <span class="o">&gt;</span> <span class="mi">2</span><span class="p">):</span>
    <span class="c1"># ...then get the length of both halves of the extra time.</span>
    <span class="n">first_extra_time</span> <span class="o">=</span> <span class="n">half_times</span><span class="p">[</span><span class="s2">&quot;minute&quot;</span><span class="p">][</span><span class="mi">2</span><span class="p">]</span> <span class="o">-</span> <span class="mi">90</span>
    <span class="n">second_extra_time</span> <span class="o">=</span> <span class="n">half_times</span><span class="p">[</span><span class="s2">&quot;minute&quot;</span><span class="p">][</span><span class="mi">3</span><span class="p">]</span> <span class="o">-</span> <span class="mi">105</span>
</pre></div>

     </div>
</div>
</div>
</div>

</div><div class="jp-Cell jp-CodeCell jp-Notebook-cell jp-mod-noOutputs  ">
<div class="jp-Cell-inputWrapper">
<div class="jp-Collapser jp-InputCollapser jp-Cell-inputCollapser">
</div>
<div class="jp-InputArea jp-Cell-inputArea">
<div class="jp-InputPrompt jp-InputArea-prompt">In&nbsp;[&nbsp;]:</div>
<div class="jp-CodeMirrorEditor jp-Editor jp-InputArea-editor" data-type="inline">
     <div class="CodeMirror cm-s-jupyter">
<div class=" highlight hl-ipython3"><pre><span></span><span class="c1"># Declare variables to use for data processing</span>
<span class="n">home_time</span> <span class="o">=</span> <span class="mi">0</span>
<span class="n">away_time</span> <span class="o">=</span> <span class="mi">0</span>
<span class="n">period</span> <span class="o">=</span> <span class="mi">0</span>
<span class="n">home_player_name</span> <span class="o">=</span> <span class="s1">&#39;&#39;</span>
<span class="n">away_player_name</span> <span class="o">=</span> <span class="s1">&#39;&#39;</span>
<span class="n">home_xg</span> <span class="o">=</span> <span class="mi">0</span>
<span class="n">away_xg</span> <span class="o">=</span> <span class="mi">0</span>

<span class="c1"># Determine the width of the gap in between each half</span>
<span class="n">gap_width</span> <span class="o">=</span> <span class="mi">3</span>
</pre></div>

     </div>
</div>
</div>
</div>

</div><div class="jp-Cell jp-CodeCell jp-Notebook-cell   ">
<div class="jp-Cell-inputWrapper">
<div class="jp-Collapser jp-InputCollapser jp-Cell-inputCollapser">
</div>
<div class="jp-InputArea jp-Cell-inputArea">
<div class="jp-InputPrompt jp-InputArea-prompt">In&nbsp;[&nbsp;]:</div>
<div class="jp-CodeMirrorEditor jp-Editor jp-InputArea-editor" data-type="inline">
     <div class="CodeMirror cm-s-jupyter">
<div class=" highlight hl-ipython3"><pre><span></span><span class="c1"># Create a blank data frame to store the xG data</span>
<span class="n">xg_data</span> <span class="o">=</span> <span class="n">pd</span><span class="o">.</span><span class="n">DataFrame</span><span class="p">()</span>

<span class="c1"># Create a sample dataset</span>
<span class="n">xGoalEvent</span> <span class="o">=</span> <span class="p">{</span>
        <span class="s1">&#39;minute&#39;</span><span class="p">:</span> <span class="mi">0</span><span class="p">,</span> <span class="c1"># Minute displayed on the xG timeline</span>
        <span class="s1">&#39;realMinute&#39;</span><span class="p">:</span> <span class="mi">0</span><span class="p">,</span> <span class="c1"># Minute that the shot took place in the match</span>
        <span class="s1">&#39;second&#39;</span><span class="p">:</span> <span class="mi">0</span><span class="p">,</span>
        <span class="s1">&#39;period&#39;</span><span class="p">:</span> <span class="mi">1</span><span class="p">,</span>
        <span class="s1">&#39;homeScorerName&#39;</span><span class="p">:</span> <span class="s1">&#39;&#39;</span><span class="p">,</span>
        <span class="s1">&#39;awayScorerName&#39;</span><span class="p">:</span> <span class="s1">&#39;&#39;</span><span class="p">,</span>
        <span class="s1">&#39;homeEachXGoal&#39;</span><span class="p">:</span> <span class="mi">0</span><span class="p">,</span> <span class="c1"># Each shot&#39;s expected goal</span>
        <span class="s1">&#39;awayEachXGoal&#39;</span><span class="p">:</span> <span class="mi">0</span><span class="p">,</span>
        <span class="s1">&#39;homeXGoal&#39;</span><span class="p">:</span> <span class="mi">0</span><span class="p">,</span> <span class="c1"># Cumulated expected goal</span>
        <span class="s1">&#39;awayXGoal&#39;</span><span class="p">:</span> <span class="mi">0</span><span class="p">,</span>
    <span class="p">}</span>

<span class="c1"># Add the sample dataset to the data frame</span>
<span class="n">xg_data</span> <span class="o">=</span> <span class="n">xg_data</span><span class="o">.</span><span class="n">append</span><span class="p">(</span><span class="n">xGoalEvent</span><span class="p">,</span> <span class="n">ignore_index</span><span class="o">=</span><span class="kc">True</span><span class="p">)</span>

<span class="c1"># Declare variables to store the individual and cumulated expected goals</span>
<span class="n">homeXGoal</span> <span class="o">=</span> <span class="mi">0</span>
<span class="n">awayXGoal</span> <span class="o">=</span> <span class="mi">0</span>
<span class="n">homeEachXGoal</span> <span class="o">=</span> <span class="mi">0</span>
<span class="n">awayEachXGoal</span> <span class="o">=</span> <span class="mi">0</span>

<span class="c1"># This loop will go through every shot events in the list.</span>
<span class="c1"># For every shot event...</span>
<span class="k">for</span> <span class="n">i</span> <span class="ow">in</span> <span class="nb">range</span><span class="p">(</span><span class="nb">len</span><span class="p">(</span><span class="n">events</span><span class="p">)):</span>
    
    <span class="c1"># Assign the real minute and second when the shot took place to the dataset</span>
    <span class="n">xGoalEvent</span><span class="p">[</span><span class="s2">&quot;realMinute&quot;</span><span class="p">]</span> <span class="o">=</span> <span class="n">events</span><span class="p">[</span><span class="s2">&quot;minute&quot;</span><span class="p">][</span><span class="n">i</span><span class="p">]</span>
    <span class="n">xGoalEvent</span><span class="p">[</span><span class="s2">&quot;second&quot;</span><span class="p">]</span> <span class="o">=</span> <span class="n">events</span><span class="p">[</span><span class="s2">&quot;second&quot;</span><span class="p">][</span><span class="n">i</span><span class="p">]</span>
    
    <span class="c1"># Check if the period of the shot event is exceeding 4 or not</span>
    <span class="c1"># Why? Because we are only looking for shots that were taken</span>
    <span class="c1"># inside the normal halves and the extra time halves,</span>
    <span class="c1"># not during the penalty shootout.</span>
    <span class="k">if</span> <span class="n">events</span><span class="p">[</span><span class="s2">&quot;period&quot;</span><span class="p">][</span><span class="n">i</span><span class="p">]</span> <span class="o">&lt;=</span> <span class="mi">4</span><span class="p">:</span>
        <span class="c1"># Assign the period of the shot to the corresponding column of the dataset</span>
        <span class="n">xGoalEvent</span><span class="p">[</span><span class="s2">&quot;period&quot;</span><span class="p">]</span> <span class="o">=</span> <span class="n">events</span><span class="p">[</span><span class="s2">&quot;period&quot;</span><span class="p">][</span><span class="n">i</span><span class="p">]</span>
        
        <span class="c1"># First tricky part here!</span>
        <span class="c1"># We need to calculate the minute which the shot will be displayed in the xG timeline.</span>
        <span class="k">if</span> <span class="n">events</span><span class="p">[</span><span class="s2">&quot;period&quot;</span><span class="p">][</span><span class="n">i</span><span class="p">]</span> <span class="o">==</span> <span class="mi">1</span><span class="p">:</span> <span class="c1"># If the shot took place in the first half...</span>
            <span class="c1"># ...assign the usual minute to the corresponding column of the dataset.</span>
            <span class="n">xGoalEvent</span><span class="p">[</span><span class="s2">&quot;minute&quot;</span><span class="p">]</span> <span class="o">=</span> <span class="n">events</span><span class="p">[</span><span class="s2">&quot;minute&quot;</span><span class="p">][</span><span class="n">i</span><span class="p">]</span>
            
        <span class="k">elif</span> <span class="n">events</span><span class="p">[</span><span class="s2">&quot;period&quot;</span><span class="p">][</span><span class="n">i</span><span class="p">]</span> <span class="o">==</span> <span class="mi">2</span><span class="p">:</span> <span class="c1"># If the shot took place in the second half...</span>
            <span class="c1"># ...add the length of the stoppage/injury time of the first half (first_half_time - 45)</span>
            <span class="c1"># and the width of the gap to the original minute when the shot took place.</span>
            <span class="n">xGoalEvent</span><span class="p">[</span><span class="s2">&quot;minute&quot;</span><span class="p">]</span> <span class="o">=</span> <span class="n">events</span><span class="p">[</span><span class="s2">&quot;minute&quot;</span><span class="p">][</span><span class="n">i</span><span class="p">]</span> <span class="o">+</span> <span class="n">first_half_time</span> <span class="o">-</span> <span class="mi">45</span> <span class="o">+</span> <span class="n">gap_width</span>
            
        <span class="k">elif</span> <span class="n">events</span><span class="p">[</span><span class="s2">&quot;period&quot;</span><span class="p">][</span><span class="n">i</span><span class="p">]</span> <span class="o">==</span> <span class="mi">3</span><span class="p">:</span> <span class="c1"># If the shot took place in the first half of the extra time...</span>
            <span class="c1"># ...do a similar process to the shots taken place in the second half.</span>
            <span class="c1"># Add the length of the stoppage/injury time of the first half *and* the second half (first_half_time - 45) and (second_half_time - 45)</span>
            <span class="c1"># and twice the width of the gap (because there are two gaps separating three halves)</span>
            <span class="c1"># to the original minute when the shot took place.</span>
            <span class="n">xGoalEvent</span><span class="p">[</span><span class="s2">&quot;minute&quot;</span><span class="p">]</span> <span class="o">=</span> <span class="n">events</span><span class="p">[</span><span class="s2">&quot;minute&quot;</span><span class="p">][</span><span class="n">i</span><span class="p">]</span> <span class="o">+</span> <span class="n">first_half_time</span> <span class="o">-</span> <span class="mi">45</span> <span class="o">+</span> <span class="n">gap_width</span> <span class="o">+</span> <span class="n">second_half_time</span> <span class="o">-</span> <span class="mi">45</span> <span class="o">+</span> <span class="n">gap_width</span>
            
        <span class="k">elif</span> <span class="n">events</span><span class="p">[</span><span class="s2">&quot;period&quot;</span><span class="p">][</span><span class="n">i</span><span class="p">]</span> <span class="o">==</span> <span class="mi">4</span><span class="p">:</span> <span class="c1"># If the shot took place in the second half of the extra time...</span>
            <span class="c1"># ...add the length of the stoppage/injury time of the first half, the second half and the first half of the extra time</span>
            <span class="c1"># (first_half_time - 45), (second_half_time - 45) and (first_extra_time - 15)</span>
            <span class="c1"># to the original minute when the shot took place.</span>
            <span class="n">xGoalEvent</span><span class="p">[</span><span class="s2">&quot;minute&quot;</span><span class="p">]</span> <span class="o">=</span> <span class="n">events</span><span class="p">[</span><span class="s2">&quot;minute&quot;</span><span class="p">][</span><span class="n">i</span><span class="p">]</span> <span class="o">+</span> <span class="n">first_half_time</span> <span class="o">-</span> <span class="mi">45</span> <span class="o">+</span> <span class="n">gap_width</span> <span class="o">+</span> <span class="n">second_half_time</span> <span class="o">-</span> <span class="mi">45</span> <span class="o">+</span> <span class="n">gap_width</span> <span class="o">+</span> \
                <span class="n">first_extra_time</span> <span class="o">-</span> <span class="mi">15</span> <span class="o">+</span> <span class="n">gap_width</span>
    
    <span class="c1"># If the period when the shot took place exceeded 4 (into the penalty shootout)</span>
    <span class="c1"># then stop the for loop.</span>
    <span class="k">else</span><span class="p">:</span>
        <span class="k">break</span>
    
    <span class="c1"># Check if the team in possession&#39;s ID matches the home team&#39;s ID or not</span>
    <span class="c1"># (Essentially check if the team tagged for this event is the home team or not)</span>
    <span class="k">if</span> <span class="n">events</span><span class="p">[</span><span class="s2">&quot;possession_team_id&quot;</span><span class="p">][</span><span class="n">i</span><span class="p">]</span> <span class="o">==</span> <span class="n">homeTeamId</span><span class="p">:</span>
        
        <span class="c1"># Check if the event is tagged as a goal or not</span>
        <span class="c1"># (Statsbomb assign every goal to ID 97. Referred to Statsbomb&#39;s doc)</span>
        <span class="c1"># (Statsbomb&#39;s doc: https://github.com/statsbomb/open-data/blob/master/doc/StatsBomb%20Open%20Data%20Specification%20v1.1.pdf)</span>
        <span class="k">if</span> <span class="n">events</span><span class="p">[</span><span class="s2">&quot;shot&quot;</span><span class="p">][</span><span class="n">i</span><span class="p">][</span><span class="s2">&quot;outcome&quot;</span><span class="p">][</span><span class="s2">&quot;id&quot;</span><span class="p">]</span> <span class="o">==</span> <span class="mi">97</span><span class="p">:</span>
            
            <span class="c1"># A for loop to go through the home team&#39;s lineup list...</span>
            <span class="k">for</span> <span class="n">y</span> <span class="ow">in</span> <span class="nb">range</span><span class="p">(</span><span class="nb">len</span><span class="p">(</span><span class="n">home_lineup</span><span class="p">)):</span>
                
                <span class="c1"># ...to check the tagged player&#39;s id...</span>
                <span class="k">if</span> <span class="n">events</span><span class="p">[</span><span class="s2">&quot;player_id&quot;</span><span class="p">][</span><span class="n">i</span><span class="p">]</span> <span class="o">==</span> <span class="n">home_lineup</span><span class="p">[</span><span class="s2">&quot;player_id&quot;</span><span class="p">][</span><span class="n">y</span><span class="p">]:</span>
                    
                    <span class="c1"># ...and to check if the tagged player has a common name/nickname or not.</span>
                    <span class="k">if</span> <span class="n">home_lineup</span><span class="p">[</span><span class="s2">&quot;player_nickname&quot;</span><span class="p">][</span><span class="n">y</span><span class="p">]</span> <span class="o">==</span> <span class="kc">None</span><span class="p">:</span> <span class="c1"># If the player does not have a nickname...</span>
                        <span class="c1"># ...assign the player&#39;s full name to the corresponding column in the dataset.</span>
                        <span class="n">xGoalEvent</span><span class="p">[</span><span class="s2">&quot;homeScorerName&quot;</span><span class="p">]</span> <span class="o">=</span> <span class="n">home_lineup</span><span class="p">[</span><span class="s2">&quot;player_name&quot;</span><span class="p">][</span><span class="n">y</span><span class="p">]</span>
                    <span class="k">else</span><span class="p">:</span> <span class="c1"># If the player has a nickname...</span>
                        <span class="c1"># ...assign the player&#39;s nickname to the corresponding column in the dataset.</span>
                        <span class="n">xGoalEvent</span><span class="p">[</span><span class="s2">&quot;homeScorerName&quot;</span><span class="p">]</span> <span class="o">=</span> <span class="n">home_lineup</span><span class="p">[</span><span class="s2">&quot;player_nickname&quot;</span><span class="p">][</span><span class="n">y</span><span class="p">]</span>
                    
                    <span class="c1"># Because the goal scorer belongs to the home team,</span>
                    <span class="c1"># we leave the name of the away&#39;s goal scorer blank.    </span>
                    <span class="n">xGoalEvent</span><span class="p">[</span><span class="s2">&quot;awayScorerName&quot;</span><span class="p">]</span> <span class="o">=</span> <span class="s1">&#39;&#39;</span>
        
        <span class="c1"># If the event is not a goal...            </span>
        <span class="k">else</span><span class="p">:</span>
            <span class="c1"># ...then leave both of the scorer columns blank.</span>
            <span class="n">xGoalEvent</span><span class="p">[</span><span class="s2">&quot;homeScorerName&quot;</span><span class="p">]</span> <span class="o">=</span> <span class="s1">&#39;&#39;</span>
            <span class="n">xGoalEvent</span><span class="p">[</span><span class="s2">&quot;awayScorerName&quot;</span><span class="p">]</span> <span class="o">=</span> <span class="s1">&#39;&#39;</span>
        
        <span class="c1"># Retrieve and convert Statsbomb&#39;s xG of the shot/goal to a float value</span>
        <span class="n">xGoalEvent</span><span class="p">[</span><span class="s2">&quot;homeEachXGoal&quot;</span><span class="p">]</span> <span class="o">=</span> <span class="nb">float</span><span class="p">(</span><span class="n">events</span><span class="p">[</span><span class="s2">&quot;shot&quot;</span><span class="p">][</span><span class="n">i</span><span class="p">][</span><span class="s2">&quot;statsbomb_xg&quot;</span><span class="p">])</span>
        <span class="n">xGoalEvent</span><span class="p">[</span><span class="s2">&quot;awayEachXGoal&quot;</span><span class="p">]</span> <span class="o">=</span> <span class="mi">0</span> <span class="c1"># We assign 0 here because the event belongs to the home team.</span>
        
        <span class="c1"># Add this event&#39;s xG on top of the xG sum of the previous events</span>
        <span class="n">homeXGoal</span> <span class="o">+=</span> <span class="nb">float</span><span class="p">(</span><span class="n">events</span><span class="p">[</span><span class="s2">&quot;shot&quot;</span><span class="p">][</span><span class="n">i</span><span class="p">][</span><span class="s2">&quot;statsbomb_xg&quot;</span><span class="p">])</span>
        <span class="c1"># Assign the new cumulated xG to the corresponding column in the dataset.</span>
        <span class="n">xGoalEvent</span><span class="p">[</span><span class="s2">&quot;homeXGoal&quot;</span><span class="p">]</span> <span class="o">=</span> <span class="n">homeXGoal</span>
    
    <span class="c1"># If the shot event belongs to the away team,</span>
    <span class="c1"># then we repeat the steps that we have done above</span>
    <span class="c1"># but only reverse the role of the home team and the away team.    </span>
    <span class="k">else</span><span class="p">:</span>
        
        <span class="k">if</span> <span class="n">events</span><span class="p">[</span><span class="s2">&quot;shot&quot;</span><span class="p">][</span><span class="n">i</span><span class="p">][</span><span class="s2">&quot;outcome&quot;</span><span class="p">][</span><span class="s2">&quot;id&quot;</span><span class="p">]</span> <span class="o">==</span> <span class="mi">97</span><span class="p">:</span>
            
            <span class="k">for</span> <span class="n">y</span> <span class="ow">in</span> <span class="nb">range</span><span class="p">(</span><span class="nb">len</span><span class="p">(</span><span class="n">away_lineup</span><span class="p">)):</span>
                
                <span class="k">if</span> <span class="n">events</span><span class="p">[</span><span class="s2">&quot;player_id&quot;</span><span class="p">][</span><span class="n">i</span><span class="p">]</span> <span class="o">==</span> <span class="n">away_lineup</span><span class="p">[</span><span class="s2">&quot;player_id&quot;</span><span class="p">][</span><span class="n">y</span><span class="p">]:</span>
                    
                    <span class="k">if</span> <span class="n">away_lineup</span><span class="p">[</span><span class="s2">&quot;player_nickname&quot;</span><span class="p">][</span><span class="n">y</span><span class="p">]</span> <span class="o">==</span> <span class="kc">None</span><span class="p">:</span>
                        <span class="n">xGoalEvent</span><span class="p">[</span><span class="s2">&quot;awayScorerName&quot;</span><span class="p">]</span> <span class="o">=</span> <span class="n">away_lineup</span><span class="p">[</span><span class="s2">&quot;player_name&quot;</span><span class="p">][</span><span class="n">y</span><span class="p">]</span>
                    <span class="k">else</span><span class="p">:</span>
                        <span class="n">xGoalEvent</span><span class="p">[</span><span class="s2">&quot;awayScorerName&quot;</span><span class="p">]</span> <span class="o">=</span> <span class="n">away_lineup</span><span class="p">[</span><span class="s2">&quot;player_nickname&quot;</span><span class="p">][</span><span class="n">y</span><span class="p">]</span>
                    
                    <span class="c1"># Like this one. Rather than leaving the away scorer&#39;s name blank,</span>
                    <span class="c1"># we now leave the home scorer&#39;s name blank because the event</span>
                    <span class="c1"># belongs to the away team.    </span>
                    <span class="n">xGoalEvent</span><span class="p">[</span><span class="s2">&quot;homeScorerName&quot;</span><span class="p">]</span> <span class="o">=</span> <span class="s1">&#39;&#39;</span>
                    
        <span class="k">else</span><span class="p">:</span>
            <span class="n">xGoalEvent</span><span class="p">[</span><span class="s2">&quot;homeScorerName&quot;</span><span class="p">]</span> <span class="o">=</span> <span class="s1">&#39;&#39;</span>
            <span class="n">xGoalEvent</span><span class="p">[</span><span class="s2">&quot;awayScorerName&quot;</span><span class="p">]</span> <span class="o">=</span> <span class="s1">&#39;&#39;</span>
        
        <span class="n">xGoalEvent</span><span class="p">[</span><span class="s2">&quot;awayEachXGoal&quot;</span><span class="p">]</span> <span class="o">=</span> <span class="nb">float</span><span class="p">(</span><span class="n">events</span><span class="p">[</span><span class="s2">&quot;shot&quot;</span><span class="p">][</span><span class="n">i</span><span class="p">][</span><span class="s2">&quot;statsbomb_xg&quot;</span><span class="p">])</span>
        <span class="n">xGoalEvent</span><span class="p">[</span><span class="s2">&quot;homeEachXGoal&quot;</span><span class="p">]</span> <span class="o">=</span> <span class="mi">0</span>
        <span class="n">awayXGoal</span> <span class="o">+=</span> <span class="nb">float</span><span class="p">(</span><span class="n">events</span><span class="p">[</span><span class="s2">&quot;shot&quot;</span><span class="p">][</span><span class="n">i</span><span class="p">][</span><span class="s2">&quot;statsbomb_xg&quot;</span><span class="p">])</span>
    
    <span class="c1"># Assign the total xG of both teams after this event</span>
    <span class="c1"># to the corresponding columns of the dataset.</span>
    <span class="n">xGoalEvent</span><span class="p">[</span><span class="s1">&#39;homeXGoal&#39;</span><span class="p">]</span> <span class="o">=</span> <span class="n">homeXGoal</span>
    <span class="n">xGoalEvent</span><span class="p">[</span><span class="s1">&#39;awayXGoal&#39;</span><span class="p">]</span> <span class="o">=</span> <span class="n">awayXGoal</span>
    
    <span class="nb">print</span><span class="p">(</span><span class="n">xGoalEvent</span><span class="p">)</span> <span class="c1"># Print out each shot event to check the gathered information</span>
    <span class="n">xg_data</span> <span class="o">=</span> <span class="n">xg_data</span><span class="o">.</span><span class="n">append</span><span class="p">(</span><span class="n">xGoalEvent</span><span class="p">,</span> <span class="n">ignore_index</span><span class="o">=</span><span class="kc">True</span><span class="p">)</span> <span class="c1"># Add each event to the big dataframe.</span>
    
<span class="c1"># The result will be printed out below, like this |</span>
<span class="c1">#                                                 v </span>
</pre></div>

     </div>
</div>
</div>
