<div class="jp-CodeMirrorEditor jp-Editor jp-InputArea-editor" data-type="inline">
     <div class="CodeMirror cm-s-jupyter">
<div class=" highlight hl-ipython3"><pre><span></span><span class="c1"># Create two dataframes,</span>
<span class="c1"># one to store the home team&#39;s shots</span>
<span class="c1"># and the other to store the away team&#39;s shots</span>
<span class="n">home_shots</span> <span class="o">=</span> <span class="n">pd</span><span class="o">.</span><span class="n">DataFrame</span><span class="p">()</span>
<span class="n">away_shots</span> <span class="o">=</span> <span class="n">pd</span><span class="o">.</span><span class="n">DataFrame</span><span class="p">()</span>

<span class="c1"># Create two sample datasets</span>
<span class="n">homeShotEvent</span> <span class="o">=</span> <span class="p">{</span>
        <span class="s1">&#39;period&#39;</span><span class="p">:</span> <span class="mi">1</span><span class="p">,</span>
        <span class="s1">&#39;shotType&#39;</span><span class="p">:</span> <span class="mi">0</span><span class="p">,</span>
        <span class="s1">&#39;xValue&#39;</span><span class="p">:</span> <span class="mi">0</span><span class="p">,</span>
        <span class="s1">&#39;yValue&#39;</span><span class="p">:</span> <span class="mi">0</span><span class="p">,</span>
        <span class="s1">&#39;playerName&#39;</span><span class="p">:</span> <span class="s1">&#39;&#39;</span><span class="p">,</span>
        <span class="s1">&#39;xGValue&#39;</span><span class="p">:</span> <span class="mi">0</span>
    <span class="p">}</span>

<span class="n">awayShotEvent</span> <span class="o">=</span> <span class="p">{</span>
        <span class="s1">&#39;period&#39;</span><span class="p">:</span> <span class="mi">1</span><span class="p">,</span>
        <span class="s1">&#39;shotType&#39;</span><span class="p">:</span> <span class="mi">0</span><span class="p">,</span>
        <span class="s1">&#39;xValue&#39;</span><span class="p">:</span> <span class="mi">0</span><span class="p">,</span>
        <span class="s1">&#39;yValue&#39;</span><span class="p">:</span> <span class="mi">0</span><span class="p">,</span>
        <span class="s1">&#39;playerName&#39;</span><span class="p">:</span> <span class="s1">&#39;&#39;</span><span class="p">,</span>
        <span class="s1">&#39;xGValue&#39;</span><span class="p">:</span> <span class="mi">0</span>
    <span class="p">}</span>

<span class="c1"># A for loop to go through the events dataframe</span>
<span class="k">for</span> <span class="n">i</span> <span class="ow">in</span> <span class="nb">range</span><span class="p">(</span><span class="nb">len</span><span class="p">(</span><span class="n">events</span><span class="p">)):</span>
    
    <span class="c1"># Check if the shot event belongs to the home team or not</span>
    <span class="c1"># If it is...</span>
    <span class="k">if</span> <span class="p">(</span><span class="n">events</span><span class="p">[</span><span class="s2">&quot;possession_team_id&quot;</span><span class="p">][</span><span class="n">i</span><span class="p">]</span> <span class="o">==</span> <span class="n">homeTeamId</span><span class="p">):</span>
        
        <span class="c1"># ...then continue checking if the event took place in the first 4 periods or not</span>
        <span class="c1"># (Remove events that taken place in the penalty shootout)</span>
        <span class="k">if</span> <span class="p">(</span><span class="n">events</span><span class="p">[</span><span class="s2">&quot;period&quot;</span><span class="p">][</span><span class="n">i</span><span class="p">]</span> <span class="o">&lt;=</span> <span class="mi">4</span><span class="p">):</span>
            <span class="n">homeShotEvent</span><span class="p">[</span><span class="s2">&quot;period&quot;</span><span class="p">]</span> <span class="o">=</span> <span class="n">events</span><span class="p">[</span><span class="s2">&quot;period&quot;</span><span class="p">][</span><span class="n">i</span><span class="p">]</span>
        <span class="k">else</span><span class="p">:</span>
            <span class="k">break</span>
        
        <span class="c1"># Assign the value to the corresponding column of the home team&#39;s dataset</span>
        <span class="n">homeShotEvent</span><span class="p">[</span><span class="s2">&quot;shotType&quot;</span><span class="p">]</span> <span class="o">=</span> <span class="n">events</span><span class="p">[</span><span class="s2">&quot;shot&quot;</span><span class="p">][</span><span class="n">i</span><span class="p">][</span><span class="s2">&quot;outcome&quot;</span><span class="p">][</span><span class="s2">&quot;id&quot;</span><span class="p">]</span>
        <span class="n">homeShotEvent</span><span class="p">[</span><span class="s2">&quot;xValue&quot;</span><span class="p">]</span> <span class="o">=</span> <span class="n">events</span><span class="p">[</span><span class="s2">&quot;location&quot;</span><span class="p">][</span><span class="n">i</span><span class="p">][</span><span class="mi">0</span><span class="p">]</span>
        <span class="n">homeShotEvent</span><span class="p">[</span><span class="s2">&quot;yValue&quot;</span><span class="p">]</span> <span class="o">=</span> <span class="n">events</span><span class="p">[</span><span class="s2">&quot;location&quot;</span><span class="p">][</span><span class="n">i</span><span class="p">][</span><span class="mi">1</span><span class="p">]</span>
        <span class="n">homeShotEvent</span><span class="p">[</span><span class="s2">&quot;xGValue&quot;</span><span class="p">]</span> <span class="o">=</span> <span class="n">events</span><span class="p">[</span><span class="s2">&quot;shot&quot;</span><span class="p">][</span><span class="n">i</span><span class="p">][</span><span class="s2">&quot;statsbomb_xg&quot;</span><span class="p">]</span>
        
        <span class="c1"># Once again, check if the tagged player has a nickname from the lineup file or not</span>
        <span class="k">for</span> <span class="n">y</span> <span class="ow">in</span> <span class="nb">range</span><span class="p">(</span><span class="nb">len</span><span class="p">(</span><span class="n">home_lineup</span><span class="p">)):</span>
            
            <span class="k">if</span> <span class="n">events</span><span class="p">[</span><span class="s2">&quot;player_id&quot;</span><span class="p">][</span><span class="n">i</span><span class="p">]</span> <span class="o">==</span> <span class="n">home_lineup</span><span class="p">[</span><span class="s2">&quot;player_id&quot;</span><span class="p">][</span><span class="n">y</span><span class="p">]:</span>
                
                <span class="k">if</span> <span class="n">home_lineup</span><span class="p">[</span><span class="s2">&quot;player_nickname&quot;</span><span class="p">][</span><span class="n">y</span><span class="p">]</span> <span class="o">==</span> <span class="kc">None</span><span class="p">:</span>
                    <span class="n">homeShotEvent</span><span class="p">[</span><span class="s2">&quot;playerName&quot;</span><span class="p">]</span> <span class="o">=</span> <span class="n">home_lineup</span><span class="p">[</span><span class="s2">&quot;player_name&quot;</span><span class="p">][</span><span class="n">y</span><span class="p">]</span>
                <span class="k">else</span><span class="p">:</span>
                    <span class="n">homeShotEvent</span><span class="p">[</span><span class="s2">&quot;playerName&quot;</span><span class="p">]</span> <span class="o">=</span> <span class="n">home_lineup</span><span class="p">[</span><span class="s2">&quot;player_nickname&quot;</span><span class="p">][</span><span class="n">y</span><span class="p">]</span>
        
        <span class="c1"># Add the shot event to the dataframe            </span>
        <span class="n">home_shots</span> <span class="o">=</span> <span class="n">home_shots</span><span class="o">.</span><span class="n">append</span><span class="p">(</span><span class="n">homeShotEvent</span><span class="p">,</span> <span class="n">ignore_index</span> <span class="o">=</span> <span class="kc">True</span><span class="p">)</span>
    
    <span class="c1"># If the event belongs to the away team</span>
    <span class="c1"># then repeat the above steps, but add the necessary values</span>
    <span class="c1"># to the away team&#39;s dataframe    </span>
    <span class="k">else</span><span class="p">:</span>
        
        <span class="k">if</span> <span class="p">(</span><span class="n">events</span><span class="p">[</span><span class="s2">&quot;period&quot;</span><span class="p">][</span><span class="n">i</span><span class="p">]</span> <span class="o">&lt;=</span> <span class="mi">4</span><span class="p">):</span>
            <span class="n">awayShotEvent</span><span class="p">[</span><span class="s2">&quot;period&quot;</span><span class="p">]</span> <span class="o">=</span> <span class="n">events</span><span class="p">[</span><span class="s2">&quot;period&quot;</span><span class="p">][</span><span class="n">i</span><span class="p">]</span>
        <span class="k">else</span><span class="p">:</span>
            <span class="k">break</span>
        
        <span class="n">awayShotEvent</span><span class="p">[</span><span class="s2">&quot;shotType&quot;</span><span class="p">]</span> <span class="o">=</span> <span class="n">events</span><span class="p">[</span><span class="s2">&quot;shot&quot;</span><span class="p">][</span><span class="n">i</span><span class="p">][</span><span class="s2">&quot;outcome&quot;</span><span class="p">][</span><span class="s2">&quot;id&quot;</span><span class="p">]</span>
        <span class="n">awayShotEvent</span><span class="p">[</span><span class="s2">&quot;xValue&quot;</span><span class="p">]</span> <span class="o">=</span> <span class="n">events</span><span class="p">[</span><span class="s2">&quot;location&quot;</span><span class="p">][</span><span class="n">i</span><span class="p">][</span><span class="mi">0</span><span class="p">]</span>
        <span class="n">awayShotEvent</span><span class="p">[</span><span class="s2">&quot;yValue&quot;</span><span class="p">]</span> <span class="o">=</span> <span class="n">events</span><span class="p">[</span><span class="s2">&quot;location&quot;</span><span class="p">][</span><span class="n">i</span><span class="p">][</span><span class="mi">1</span><span class="p">]</span>
        <span class="n">awayShotEvent</span><span class="p">[</span><span class="s2">&quot;xGValue&quot;</span><span class="p">]</span> <span class="o">=</span> <span class="n">events</span><span class="p">[</span><span class="s2">&quot;shot&quot;</span><span class="p">][</span><span class="n">i</span><span class="p">][</span><span class="s2">&quot;statsbomb_xg&quot;</span><span class="p">]</span>
        
        <span class="k">for</span> <span class="n">y</span> <span class="ow">in</span> <span class="nb">range</span><span class="p">(</span><span class="nb">len</span><span class="p">(</span><span class="n">away_lineup</span><span class="p">)):</span>
            
            <span class="k">if</span> <span class="n">events</span><span class="p">[</span><span class="s2">&quot;player_id&quot;</span><span class="p">][</span><span class="n">i</span><span class="p">]</span> <span class="o">==</span> <span class="n">away_lineup</span><span class="p">[</span><span class="s2">&quot;player_id&quot;</span><span class="p">][</span><span class="n">y</span><span class="p">]:</span>
                
                <span class="k">if</span> <span class="n">away_lineup</span><span class="p">[</span><span class="s2">&quot;player_nickname&quot;</span><span class="p">][</span><span class="n">y</span><span class="p">]</span> <span class="o">==</span> <span class="kc">None</span><span class="p">:</span>
                    <span class="n">awayShotEvent</span><span class="p">[</span><span class="s2">&quot;playerName&quot;</span><span class="p">]</span> <span class="o">=</span> <span class="n">away_lineup</span><span class="p">[</span><span class="s2">&quot;player_name&quot;</span><span class="p">][</span><span class="n">y</span><span class="p">]</span>
                <span class="k">else</span><span class="p">:</span>
                    <span class="n">awayShotEvent</span><span class="p">[</span><span class="s2">&quot;playerName&quot;</span><span class="p">]</span> <span class="o">=</span> <span class="n">away_lineup</span><span class="p">[</span><span class="s2">&quot;player_nickname&quot;</span><span class="p">][</span><span class="n">y</span><span class="p">]</span>

        <span class="n">away_shots</span> <span class="o">=</span> <span class="n">away_shots</span><span class="o">.</span><span class="n">append</span><span class="p">(</span><span class="n">awayShotEvent</span><span class="p">,</span> <span class="n">ignore_index</span> <span class="o">=</span> <span class="kc">True</span><span class="p">)</span>
</pre></div>

     </div>
</div>
</div>
