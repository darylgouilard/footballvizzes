     <div class="CodeMirror cm-s-jupyter">
<div class=" highlight hl-ipython3"><pre><span></span><span class="c1"># Create variables to count specific shot types</span>
<span class="n">home_goals</span> <span class="o">=</span> <span class="mi">0</span>
<span class="n">home_on_target</span> <span class="o">=</span> <span class="mi">0</span>
<span class="n">home_post</span> <span class="o">=</span> <span class="mi">0</span>
<span class="n">home_off_target</span> <span class="o">=</span> <span class="mi">0</span>
<span class="n">home_blocked</span> <span class="o">=</span> <span class="mi">0</span>

<span class="n">away_goals</span> <span class="o">=</span> <span class="mi">0</span>
<span class="n">away_on_target</span> <span class="o">=</span> <span class="mi">0</span>
<span class="n">away_post</span> <span class="o">=</span> <span class="mi">0</span>
<span class="n">away_off_target</span> <span class="o">=</span> <span class="mi">0</span>
<span class="n">away_blocked</span> <span class="o">=</span> <span class="mi">0</span>

<span class="c1"># Create the pitch using mplsoccer&#39;s package</span>
<span class="n">pitch</span> <span class="o">=</span> <span class="n">Pitch</span><span class="p">(</span><span class="n">pitch_type</span> <span class="o">=</span> <span class="s1">&#39;statsbomb&#39;</span><span class="p">,</span> <span class="n">pitch_color</span> <span class="o">=</span> <span class="s1">&#39;none&#39;</span><span class="p">,</span> <span class="n">line_color</span> <span class="o">=</span> <span class="s1">&#39;white&#39;</span><span class="p">,</span> <span class="n">constrained_layout</span> <span class="o">=</span> <span class="kc">True</span><span class="p">,</span> <span class="n">tight_layout</span> <span class="o">=</span> <span class="kc">True</span><span class="p">)</span>
<span class="c1"># Draw the pitch</span>
<span class="c1"># (figsize determines the width and height of the plot respectively)</span>
<span class="n">fig</span><span class="p">,</span> <span class="n">ax</span> <span class="o">=</span> <span class="n">pitch</span><span class="o">.</span><span class="n">draw</span><span class="p">(</span><span class="n">figsize</span> <span class="o">=</span> <span class="p">(</span><span class="mi">10</span><span class="p">,</span> <span class="mi">8</span><span class="p">))</span>

<span class="c1"># A for loop to go through the home team&#39;s shot events</span>
<span class="k">for</span> <span class="n">i</span> <span class="ow">in</span> <span class="nb">range</span><span class="p">(</span><span class="nb">len</span><span class="p">(</span><span class="n">home_shots</span><span class="p">)):</span>

    <span class="c1"># If the shot event is tagged as a goal...</span>
    <span class="k">if</span> <span class="p">(</span><span class="n">home_shots</span><span class="p">[</span><span class="s2">&quot;shotType&quot;</span><span class="p">][</span><span class="n">i</span><span class="p">]</span> <span class="o">==</span> <span class="mi">97</span><span class="p">):</span>
        <span class="c1"># ...draw the dot on the pitch to indicate where the shot was made.</span>
        <span class="c1"># The nitty gritty calculation is for moving the dot from the right side of the pitch</span>
        <span class="c1"># to the left side of the pitch, since the shots are made by the home team.</span>
        <span class="n">nodes</span> <span class="o">=</span> <span class="n">pitch</span><span class="o">.</span><span class="n">scatter</span><span class="p">(</span><span class="n">home_shots</span><span class="p">[</span><span class="s2">&quot;xValue&quot;</span><span class="p">][</span><span class="n">i</span><span class="p">]</span> <span class="o">-</span> <span class="p">((</span><span class="n">home_shots</span><span class="p">[</span><span class="s2">&quot;xValue&quot;</span><span class="p">][</span><span class="n">i</span><span class="p">]</span> <span class="o">-</span> <span class="mi">60</span><span class="p">)</span> <span class="o">*</span> <span class="mi">2</span><span class="p">),</span> <span class="n">home_shots</span><span class="p">[</span><span class="s2">&quot;yValue&quot;</span><span class="p">][</span><span class="n">i</span><span class="p">]</span> <span class="o">-</span> <span class="p">((</span><span class="n">home_shots</span><span class="p">[</span><span class="s2">&quot;yValue&quot;</span><span class="p">][</span><span class="n">i</span><span class="p">]</span> <span class="o">-</span> <span class="mi">40</span><span class="p">)</span> <span class="o">*</span> <span class="mi">2</span><span class="p">),</span> 
                              <span class="n">s</span> <span class="o">=</span> <span class="mi">700</span> <span class="o">*</span> <span class="n">home_shots</span><span class="p">[</span><span class="s2">&quot;xGValue&quot;</span><span class="p">][</span><span class="n">i</span><span class="p">],</span> <span class="n">marker</span> <span class="o">=</span> <span class="s1">&#39;o&#39;</span><span class="p">,</span> <span class="n">color</span> <span class="o">=</span> <span class="n">home_colour</span><span class="p">,</span> <span class="n">edgecolors</span> <span class="o">=</span> <span class="s1">&#39;yellow&#39;</span><span class="p">,</span> <span class="n">zorder</span> <span class="o">=</span> <span class="mi">1</span><span class="p">,</span> <span class="n">ax</span> <span class="o">=</span> <span class="n">ax</span><span class="p">)</span>
        <span class="c1"># Add one to the total amount of goals and shots on target of the home team</span>
        <span class="n">home_goals</span> <span class="o">=</span> <span class="n">home_goals</span> <span class="o">+</span> <span class="mi">1</span>
        <span class="n">home_on_target</span> <span class="o">=</span> <span class="n">home_on_target</span> <span class="o">+</span> <span class="mi">1</span>
    
    <span class="c1"># If the shot event is tagged as a shot on target...</span>
    <span class="k">elif</span> <span class="p">(</span><span class="n">home_shots</span><span class="p">[</span><span class="s2">&quot;shotType&quot;</span><span class="p">][</span><span class="n">i</span><span class="p">]</span> <span class="o">==</span> <span class="mi">100</span><span class="p">):</span>
        <span class="c1"># ...then draw the dot with a different shape on the pitch to indicate where the shot was made.</span>
        <span class="n">nodes</span> <span class="o">=</span> <span class="n">pitch</span><span class="o">.</span><span class="n">scatter</span><span class="p">(</span><span class="n">home_shots</span><span class="p">[</span><span class="s2">&quot;xValue&quot;</span><span class="p">][</span><span class="n">i</span><span class="p">]</span> <span class="o">-</span> <span class="p">((</span><span class="n">home_shots</span><span class="p">[</span><span class="s2">&quot;xValue&quot;</span><span class="p">][</span><span class="n">i</span><span class="p">]</span> <span class="o">-</span> <span class="mi">60</span><span class="p">)</span> <span class="o">*</span> <span class="mi">2</span><span class="p">),</span> <span class="n">home_shots</span><span class="p">[</span><span class="s2">&quot;yValue&quot;</span><span class="p">][</span><span class="n">i</span><span class="p">]</span> <span class="o">-</span> <span class="p">((</span><span class="n">home_shots</span><span class="p">[</span><span class="s2">&quot;yValue&quot;</span><span class="p">][</span><span class="n">i</span><span class="p">]</span> <span class="o">-</span> <span class="mi">40</span><span class="p">)</span> <span class="o">*</span> <span class="mi">2</span><span class="p">),</span> 
                              <span class="n">s</span> <span class="o">=</span> <span class="mi">700</span> <span class="o">*</span> <span class="n">home_shots</span><span class="p">[</span><span class="s2">&quot;xGValue&quot;</span><span class="p">][</span><span class="n">i</span><span class="p">],</span> <span class="n">marker</span> <span class="o">=</span> <span class="s1">&#39;^&#39;</span><span class="p">,</span> <span class="n">color</span> <span class="o">=</span> <span class="n">home_colour</span><span class="p">,</span> <span class="n">edgecolors</span> <span class="o">=</span> <span class="n">home_edge_colour</span><span class="p">,</span> <span class="n">zorder</span> <span class="o">=</span> <span class="mi">1</span><span class="p">,</span> <span class="n">ax</span> <span class="o">=</span> <span class="n">ax</span><span class="p">)</span>
        <span class="c1"># Add one to the total amount of shots on target of the home team</span>
        <span class="n">home_on_target</span> <span class="o">=</span> <span class="n">home_on_target</span> <span class="o">+</span> <span class="mi">1</span>
        
    <span class="c1"># (You get the point by now...)</span>
    <span class="c1"># typeId 99 is assigned to shots that hit the post, and typeId 116 is assigned to shots that are saved and then hit the post</span>
    <span class="k">elif</span> <span class="p">(</span><span class="n">home_shots</span><span class="p">[</span><span class="s2">&quot;shotType&quot;</span><span class="p">][</span><span class="n">i</span><span class="p">]</span> <span class="o">==</span> <span class="mi">99</span><span class="p">)</span> <span class="ow">or</span> <span class="p">(</span><span class="n">home_shots</span><span class="p">[</span><span class="s2">&quot;shotType&quot;</span><span class="p">][</span><span class="n">i</span><span class="p">]</span> <span class="o">==</span> <span class="mi">116</span><span class="p">):</span>
        <span class="n">nodes</span> <span class="o">=</span> <span class="n">pitch</span><span class="o">.</span><span class="n">scatter</span><span class="p">(</span><span class="n">home_shots</span><span class="p">[</span><span class="s2">&quot;xValue&quot;</span><span class="p">][</span><span class="n">i</span><span class="p">]</span> <span class="o">-</span> <span class="p">((</span><span class="n">home_shots</span><span class="p">[</span><span class="s2">&quot;xValue&quot;</span><span class="p">][</span><span class="n">i</span><span class="p">]</span> <span class="o">-</span> <span class="mi">60</span><span class="p">)</span> <span class="o">*</span> <span class="mi">2</span><span class="p">),</span> <span class="n">home_shots</span><span class="p">[</span><span class="s2">&quot;yValue&quot;</span><span class="p">][</span><span class="n">i</span><span class="p">]</span> <span class="o">-</span> <span class="p">((</span><span class="n">home_shots</span><span class="p">[</span><span class="s2">&quot;yValue&quot;</span><span class="p">][</span><span class="n">i</span><span class="p">]</span> <span class="o">-</span> <span class="mi">40</span><span class="p">)</span> <span class="o">*</span> <span class="mi">2</span><span class="p">),</span> 
                              <span class="n">s</span> <span class="o">=</span> <span class="mi">700</span> <span class="o">*</span> <span class="n">home_shots</span><span class="p">[</span><span class="s2">&quot;xGValue&quot;</span><span class="p">][</span><span class="n">i</span><span class="p">],</span> <span class="n">marker</span> <span class="o">=</span> <span class="s1">&#39;s&#39;</span><span class="p">,</span> <span class="n">color</span> <span class="o">=</span> <span class="n">home_colour</span><span class="p">,</span> <span class="n">edgecolors</span> <span class="o">=</span> <span class="n">home_edge_colour</span><span class="p">,</span> <span class="n">zorder</span> <span class="o">=</span> <span class="mi">1</span><span class="p">,</span> <span class="n">ax</span> <span class="o">=</span> <span class="n">ax</span><span class="p">)</span>
        <span class="n">home_post</span> <span class="o">=</span> <span class="n">home_post</span> <span class="o">+</span> <span class="mi">1</span>
        
    <span class="k">elif</span> <span class="p">(</span><span class="n">home_shots</span><span class="p">[</span><span class="s2">&quot;shotType&quot;</span><span class="p">][</span><span class="n">i</span><span class="p">]</span> <span class="o">==</span> <span class="mi">96</span><span class="p">):</span>
        <span class="n">nodes</span> <span class="o">=</span> <span class="n">pitch</span><span class="o">.</span><span class="n">scatter</span><span class="p">(</span><span class="n">home_shots</span><span class="p">[</span><span class="s2">&quot;xValue&quot;</span><span class="p">][</span><span class="n">i</span><span class="p">]</span> <span class="o">-</span> <span class="p">((</span><span class="n">home_shots</span><span class="p">[</span><span class="s2">&quot;xValue&quot;</span><span class="p">][</span><span class="n">i</span><span class="p">]</span> <span class="o">-</span> <span class="mi">60</span><span class="p">)</span> <span class="o">*</span> <span class="mi">2</span><span class="p">),</span> <span class="n">home_shots</span><span class="p">[</span><span class="s2">&quot;yValue&quot;</span><span class="p">][</span><span class="n">i</span><span class="p">]</span> <span class="o">-</span> <span class="p">((</span><span class="n">home_shots</span><span class="p">[</span><span class="s2">&quot;yValue&quot;</span><span class="p">][</span><span class="n">i</span><span class="p">]</span> <span class="o">-</span> <span class="mi">40</span><span class="p">)</span> <span class="o">*</span> <span class="mi">2</span><span class="p">),</span> 
                              <span class="n">s</span> <span class="o">=</span> <span class="mi">700</span> <span class="o">*</span> <span class="n">home_shots</span><span class="p">[</span><span class="s2">&quot;xGValue&quot;</span><span class="p">][</span><span class="n">i</span><span class="p">],</span> <span class="n">marker</span> <span class="o">=</span> <span class="s1">&#39;D&#39;</span><span class="p">,</span> <span class="n">color</span> <span class="o">=</span> <span class="n">home_colour</span><span class="p">,</span> <span class="n">edgecolors</span> <span class="o">=</span> <span class="n">home_edge_colour</span><span class="p">,</span> <span class="n">zorder</span> <span class="o">=</span> <span class="mi">1</span><span class="p">,</span> <span class="n">ax</span> <span class="o">=</span> <span class="n">ax</span><span class="p">)</span>
        <span class="n">home_blocked</span> <span class="o">=</span> <span class="n">home_blocked</span> <span class="o">+</span> <span class="mi">1</span>
    
    <span class="c1"># Remaining typeId are:</span>
    <span class="c1"># 98 - shots off target, 101 - unthreatening shots that might end up among the fans in the stands,</span>
    <span class="c1"># 115 - shots that should be off target, but the keepers have not been tested for a while, so the keeper decides to save it just for funsies.</span>
    <span class="k">else</span><span class="p">:</span>
        <span class="n">nodes</span> <span class="o">=</span> <span class="n">pitch</span><span class="o">.</span><span class="n">scatter</span><span class="p">(</span><span class="n">home_shots</span><span class="p">[</span><span class="s2">&quot;xValue&quot;</span><span class="p">][</span><span class="n">i</span><span class="p">]</span> <span class="o">-</span> <span class="p">((</span><span class="n">home_shots</span><span class="p">[</span><span class="s2">&quot;xValue&quot;</span><span class="p">][</span><span class="n">i</span><span class="p">]</span> <span class="o">-</span> <span class="mi">60</span><span class="p">)</span> <span class="o">*</span> <span class="mi">2</span><span class="p">),</span> <span class="n">home_shots</span><span class="p">[</span><span class="s2">&quot;yValue&quot;</span><span class="p">][</span><span class="n">i</span><span class="p">]</span> <span class="o">-</span> <span class="p">((</span><span class="n">home_shots</span><span class="p">[</span><span class="s2">&quot;yValue&quot;</span><span class="p">][</span><span class="n">i</span><span class="p">]</span> <span class="o">-</span> <span class="mi">40</span><span class="p">)</span> <span class="o">*</span> <span class="mi">2</span><span class="p">),</span> 
                              <span class="n">s</span> <span class="o">=</span> <span class="mi">700</span> <span class="o">*</span> <span class="n">home_shots</span><span class="p">[</span><span class="s2">&quot;xGValue&quot;</span><span class="p">][</span><span class="n">i</span><span class="p">],</span> <span class="n">marker</span> <span class="o">=</span> <span class="s1">&#39;X&#39;</span><span class="p">,</span> <span class="n">color</span> <span class="o">=</span> <span class="n">home_colour</span><span class="p">,</span> <span class="n">edgecolors</span> <span class="o">=</span> <span class="n">home_edge_colour</span><span class="p">,</span> <span class="n">zorder</span> <span class="o">=</span> <span class="mi">1</span><span class="p">,</span> <span class="n">ax</span> <span class="o">=</span> <span class="n">ax</span><span class="p">)</span>
        <span class="n">home_off_target</span> <span class="o">=</span> <span class="n">home_off_target</span> <span class="o">+</span> <span class="mi">1</span>
        
<span class="k">for</span> <span class="n">i</span> <span class="ow">in</span> <span class="nb">range</span><span class="p">(</span><span class="nb">len</span><span class="p">(</span><span class="n">away_shots</span><span class="p">)):</span>
    
    <span class="k">if</span> <span class="p">(</span><span class="n">away_shots</span><span class="p">[</span><span class="s2">&quot;shotType&quot;</span><span class="p">][</span><span class="n">i</span><span class="p">]</span> <span class="o">==</span> <span class="mi">97</span><span class="p">):</span>
        <span class="n">nodes</span> <span class="o">=</span> <span class="n">pitch</span><span class="o">.</span><span class="n">scatter</span><span class="p">(</span><span class="n">away_shots</span><span class="p">[</span><span class="s2">&quot;xValue&quot;</span><span class="p">][</span><span class="n">i</span><span class="p">],</span> <span class="n">away_shots</span><span class="p">[</span><span class="s2">&quot;yValue&quot;</span><span class="p">][</span><span class="n">i</span><span class="p">],</span> <span class="n">s</span> <span class="o">=</span> <span class="mi">700</span> <span class="o">*</span> <span class="n">away_shots</span><span class="p">[</span><span class="s2">&quot;xGValue&quot;</span><span class="p">][</span><span class="n">i</span><span class="p">],</span> <span class="n">marker</span> <span class="o">=</span> <span class="s1">&#39;o&#39;</span><span class="p">,</span>
                                <span class="n">color</span> <span class="o">=</span> <span class="n">away_colour</span><span class="p">,</span> <span class="n">edgecolors</span> <span class="o">=</span> <span class="s1">&#39;yellow&#39;</span><span class="p">,</span> <span class="n">zorder</span> <span class="o">=</span> <span class="mi">1</span><span class="p">,</span> <span class="n">ax</span> <span class="o">=</span> <span class="n">ax</span><span class="p">)</span>
