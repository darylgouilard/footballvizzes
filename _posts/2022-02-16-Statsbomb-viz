<div class="jp-CodeMirrorEditor jp-Editor jp-InputArea-editor" data-type="inline">
     <div class="CodeMirror cm-s-jupyter">
<div class=" highlight hl-ipython3"><pre><span></span><span class="c1"># Declare two variables to store the home team and away team&#39;s IDs</span>
<span class="n">homeTeamId</span> <span class="o">=</span> <span class="mi">0</span>
<span class="n">awayTeamId</span> <span class="o">=</span> <span class="mi">0</span>

<span class="c1"># Cross check the team&#39;s name between the match_info and events list to get the teams&#39; IDs</span>
<span class="k">while</span> <span class="p">(</span><span class="n">homeTeamId</span> <span class="o">==</span> <span class="mi">0</span><span class="p">)</span> <span class="ow">and</span> <span class="p">(</span><span class="n">awayTeamId</span> <span class="o">==</span> <span class="mi">0</span><span class="p">):</span> <span class="c1"># While both teams&#39; ID remain at 0...</span>
    
    <span class="c1"># Go through the events file...</span>
    <span class="k">for</span> <span class="n">i</span> <span class="ow">in</span> <span class="nb">range</span><span class="p">(</span><span class="nb">len</span><span class="p">(</span><span class="n">events</span><span class="p">)):</span>
        
        <span class="c1"># If the name of the team in possession matches the name of the home team...</span>
        <span class="k">if</span> <span class="p">(</span><span class="n">events</span><span class="p">[</span><span class="s2">&quot;possession_team&quot;</span><span class="p">][</span><span class="n">i</span><span class="p">]</span> <span class="o">==</span> <span class="n">homeTeam</span><span class="p">):</span>
            <span class="n">homeTeamId</span> <span class="o">=</span> <span class="n">events</span><span class="p">[</span><span class="s2">&quot;possession_team_id&quot;</span><span class="p">][</span><span class="n">i</span><span class="p">]</span> <span class="c1"># ...then assign the ID of that team to the homeTeamId variable</span>
        <span class="c1"># Or if the name of the team in possession matches the name of the away team...</span>
        <span class="k">elif</span> <span class="p">(</span><span class="n">events</span><span class="p">[</span><span class="s2">&quot;possession_team&quot;</span><span class="p">][</span><span class="n">i</span><span class="p">]</span> <span class="o">==</span> <span class="n">awayTeam</span><span class="p">):</span>
            <span class="n">awayTeamId</span> <span class="o">=</span> <span class="n">events</span><span class="p">[</span><span class="s2">&quot;possession_team_id&quot;</span><span class="p">][</span><span class="n">i</span><span class="p">]</span> <span class="c1"># ... then assign the ID of that team to the awayTeamID variable</span>
    
    <span class="c1"># At the end of the for loop, check the result and print the outcome.</span>
    <span class="c1"># If both teams&#39; IDs are still 0...</span>
    <span class="k">if</span> <span class="p">(</span><span class="n">homeTeamId</span> <span class="o">==</span> <span class="mi">0</span><span class="p">)</span> <span class="ow">and</span> <span class="p">(</span><span class="n">awayTeamId</span> <span class="o">==</span> <span class="mi">0</span><span class="p">):</span>
        <span class="nb">print</span><span class="p">(</span><span class="s2">&quot;Could not find the matching teams from the match_info file in the events file.&quot;</span><span class="p">)</span>
        <span class="k">break</span> <span class="c1"># ...then inform the coder and escape the while loop.</span>
    <span class="c1"># If the for loop found the home team&#39;s ID, but not the away team&#39;s ID...</span>
    <span class="k">elif</span> <span class="p">(</span><span class="n">homeTeamId</span> <span class="o">!=</span> <span class="mi">0</span><span class="p">)</span> <span class="ow">and</span> <span class="p">(</span><span class="n">awayTeamId</span> <span class="o">==</span> <span class="mi">0</span><span class="p">):</span>
        <span class="nb">print</span><span class="p">(</span><span class="s2">&quot;Home team&#39;s ID is &quot;</span> <span class="o">+</span> <span class="nb">str</span><span class="p">(</span><span class="n">homeTeamId</span><span class="p">)</span> <span class="o">+</span> <span class="s2">&quot; and could not find the matching away team in the events file.&quot;</span><span class="p">)</span>
        <span class="k">break</span> <span class="c1"># ... then print out the home team&#39;s ID, inform the coder the bad news, and escape the while loop.</span>
    <span class="c1"># If the for loop found the away team&#39;s ID, but not the home team&#39;s ID...</span>
    <span class="k">elif</span> <span class="p">(</span><span class="n">homeTeamId</span> <span class="o">==</span> <span class="mi">0</span><span class="p">)</span> <span class="ow">and</span> <span class="p">(</span><span class="n">awayTeamId</span> <span class="o">!=</span> <span class="mi">0</span><span class="p">):</span>
        <span class="nb">print</span><span class="p">(</span><span class="s2">&quot;Could not find the matching home team in the events file and the away team&#39;s ID is &quot;</span> <span class="o">+</span> <span class="nb">str</span><span class="p">(</span><span class="n">awayTeamId</span><span class="p">))</span>
        <span class="k">break</span> <span class="c1"># ... then inform the coder the bad news, then bring some positivity in the form of the away team&#39;s ID,</span>
              <span class="c1"># and escape the while loop.</span>
    <span class="c1"># If the for loop found both teams&#39; IDs...</span>
    <span class="k">else</span><span class="p">:</span>
        <span class="nb">print</span><span class="p">(</span><span class="s2">&quot;Home team&#39;s ID is &quot;</span> <span class="o">+</span> <span class="nb">str</span><span class="p">(</span><span class="n">homeTeamId</span><span class="p">)</span> <span class="o">+</span> <span class="s2">&quot;, away team&#39;s ID is &quot;</span> <span class="o">+</span> <span class="nb">str</span><span class="p">(</span><span class="n">awayTeamId</span><span class="p">))</span>
        <span class="k">break</span> <span class="c1"># ...then inform the coder about the good news in the form of both teams&#39; IDs and excape the while loop.</span>
</pre></div>

